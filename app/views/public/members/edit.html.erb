<h1><%= @member.member_name %>さんプロフィールを編集</h1>
<%= form_with model: @member, url: member_path, method: :patch do |f| %>
  <table>
    <tbody>
      <tr>
        <th>ニックネーム</th>
        <td><%= f.text_field :member_name %></td>
      </tr>
      <tr>
        <th>メールアドレス</th>
        <td><%= f.email_field :email %></td>
      </tr>
      <tr>
        <th>年齢</th>
        <td><%= f.text_field :age %></td>
      </tr>
      <tr>
        <th>性別</th>
        <td><%= @member.gender %></td>
      </tr>
      <tr>
        <th>自己紹介文</th>
        <td><%= f.text_field :introduction %></td>
      </tr>
    </tbody>
  </table>
  <%= f.submit "編集内容を保存" %>
<% end %>
<%= link_to "戻る", member_path(@member.id) %>


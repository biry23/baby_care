<h1>全ての投稿一覧</h1>
<table>
  <thead>
    <tr>
      <th>タイトル</th>
      <th>投稿者</th>
      <th>投稿日時</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
    <tr>
      <td><%= link_to post.title, post_path(post.id) %></td>
      <td><%= post.member.member_name %></td>
      <td><%= post.created_at %></td>
      <td>お気に入りボタン作成する</td>
    </tr>
    <% end %>
  </tbody>
</table>

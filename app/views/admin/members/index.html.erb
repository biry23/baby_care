<div class="container">
  <div class="row">
    <div class="col-md-12 text-center">
      <h1>ユーザー一覧</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12 text-center">
      <table class="table">
        <thead class="bg-light">
          <tr>
            <th>id</th>
            <th>名前</th>
            <th>メール</th>
            <th>投稿数</th>
            <th>登録日</th>
          </tr>
        </thead>
        <tbody>
          <% @members.each do |member| %>
          <tr>
            <td><%= member.id %></td>
            <td><%= link_to member.member_name, admin_member_path(member.id) %></td>
            <td><%= member.email %></td>
            <td><%= member.posts.count %></td>
            <td><%= member.created_at.strftime("%Y年%m月%d日") %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

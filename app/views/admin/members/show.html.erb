<h1><%= @member.member_name %>さんのユーザー詳細</h1>
<table>
  <tbody>
    <tr>
      <th>ユーザーid</th>
      <td><%= @member.id %></td>
    </tr>
    <tr>
      <th>メールアドレス</th>
      <td><%= @member.email %></td>
    </tr>
    <tr>
      <th>性別</th>
      <td><%= @member.gender %></td>
    </tr>
    <tr>
      <th>年齢</th>
      <td><%= @member.age %></td>
    </tr>
    <tr>
      <th>投稿数</th>
      <td><%= @member.posts.count %></td>
    </tr>
  </tbody>
</table>

